  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./scripts/start.js",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.jsx --fix src",
    "start": "node ./scripts/start.js",
    "stats": "node ./scripts/start.js --stats",
    "build": "node ./scripts/build.js",
    "test": "./node_modules/eslint/bin/eslint.js --ext .js,.jsx && jest --forceExit --runInBand",
    "jest": "jest --useStderr --forceExit --runInBand",
    "testrpc": "./node_modules/ethereumjs-testrpc/bin/testrpc",
    "bridge": "cd node_modules/ethereum-bridge && mkdir -p database && node bridge.js",
    "contracts": "cd node_modules/chronobank-smart-contracts && node ../truffle/cli.js migrate --reset --network development && node ../truffle/cli.js exec setup/* --network development",
    "tconsole": "cd node_modules/chronobank-smart-contracts && node ../truffle/cli.js console"
  },
  "jest": {
    "testURL": "http://example.com",
    "setupTestFrameworkScriptFile": "./specs/init.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@(.*)$": "<rootDir>/src/$1",
      "^%(.*)$": "<rootDir>/specs/$1",
      "\\.(css|scss)$": "<rootDir>/mocks/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|woff|otf|svg)$": "<rootDir>/mocks/fileMock.js"
    }
  }
}
