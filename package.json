  "scripts": {
    "dev": "node ./scripts/start.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .js,.jsx --debug --fix .",
    "lint:pr": "bash -c \"node node_modules/eslint/bin/eslint --format node_modules/eslint-friendly-formatter/index.js --ext .js,.jsx `git diff --name-status $(git merge-base origin/develop HEAD)..HEAD | grep -E '^[AM]' | awk ' {print $2} ' | grep -E 'jsx?$'`\"",
    "lint:report": "bash -c \"node ./node_modules/eslint/bin/eslint --format node_modules/eslint-html-reporter/reporter-team-city.js --output-file ./report/eslint-report-`date +%F`.html --debug --ext .js,.jsx .\"",
    "lint:sort": "bash -c \"node ./scripts/importSort.js `git diff --name-status $(git merge-base origin/develop HEAD)..HEAD | grep -E '^[AM]' | awk ' {print $2} ' | grep -E 'jsx?$'`\"",
    "lint:test": "node ./node_modules/eslint/bin/eslint.js --ext .js,.jsx .",
    "start": "npm run dev",
    "stats": "node ./scripts/start.js --stats",
    "build": "node ./scripts/build.js",
    "build-archive": "npm run build && node ./scripts/compress.js build build.zip",
    "standalone": "NODE_ENV=standalone node ./scripts/build.js",
    "test": "jest --useStderr --forceExit --runInBand",
    "testrpc": "./node_modules/ethereumjs-testrpc/bin/testrpc --trace_request true | grep -v eth_getFilterChanges",
    "bridge": "cd node_modules/ethereum-bridge && mkdir -p database && node bridge.js",
    "contracts:compile": "cd node_modules/chronobank-smart-contracts && node --max_old_space_size=8000 ../truffle/build/cli.bundled.js compile --all",
    "contracts": "cd node_modules/chronobank-smart-contracts && node --max_old_space_size=8000 ../truffle/build/cli.bundled.js migrate --network development",
    "tconsole": "cd node_modules/chronobank-smart-contracts && node ../truffle/build/cli.bundled.js console",
    "hs": "hs build -a localhost -o",
    "rei": "rm -rf ./node_modules && npm i && npm run contracts:compile"
  },
  "pre-commit": [],
  "jest": {
    "testURL": "http://example.com",
    "setupTestFrameworkScriptFile": "./src/specsInit.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!**/*.{spec.js,mock.js}**"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@(.*)$": "<rootDir>/src/$1",
      "\\.(css|scss)$": "<rootDir>/mocks/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|woff|otf|svg)$": "<rootDir>/mocks/fileMock.js"
    }
  }
