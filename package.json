    "start": "node ./scripts/start.js",
    "stats": "node ./scripts/start.js --stats",
    "build": "node ./scripts/build.js",
    "test": "standard --fix && jest --forceExit --runInBand",
    "bridge": "cd node_modules/ethereum-bridge && mkdir -p database && node bridge.js -H localhost:8545",
