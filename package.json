    "start": "node ./scripts/start.js",
    "stats": "node ./scripts/start.js --stats",
    "build": "node ./scripts/build.js",
    "test": "standard --fix && jest --forceExit --runInBand",
    "bridge": "cd node_modules/ethereum-bridge && mkdir -p database && node bridge.js -H localhost:8545",
    "contracts": "cd node_modules/chronobank-smart-contracts && node ../truffle/cli.js migrate --reset --network development && node ../truffle/cli.js exec setup/* --network development && cd ../../",
